{
	"manifest_version":2,

	"name":"Second Research Google Chrome Extension",
	"description":"This is google chrome extension learning",
	"version":"1.0",

	"browser_action":{
		"default_icon":"icon.png",
		"default_popup":"popup.html"
	},

	"icons":{
		"16":"icon-16.png",
		"48":"icon-48.png",
		"64":"icon-64.png",
		"128":"icon-128.png"
	},

	"background":{
		"scripts":["jquery.js","background.js"],
		"persistent":true
	},

	"permissions":[
		"contextMenus"
	]
}